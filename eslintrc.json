{
  "extends": "eslint-config-airbnb",
  "rules": {
    "semi": [2, "never"],
    "eqeqeq": [2, "smart"],
    "no-eq-null": 0,
    "brace-style": [2, "stroustrup"],
    "no-use-before-define": [2, "nofunc"],
    "react/jsx-uses-react": 2,
    "block-scoped-var": 0,
    "no-param-reassign": 0,
    "guard-for-in": 0,
    "no-unexpected-multiline": 2,
    "comma-dangle": 0
  },
  "plugins": [
    "react"
  ],
  "env": {
    "browser": true,
    "mocha": true
  },
  "globals": {
    "__DEV__": true,
    "__TEST__": true,
    "process": true
  }
}
